        "@vue/cli-service": "~5.0.0",
        "bumpp": "^8.2.1",
        "eslint": "^8.31.0",
        "rollup": "^3.9.1",
        "rollup-plugin-typescript2": "^0.34.1",
        "typescript": "^4.9.4"
    }
