{
    "name": "vue-cli-plugin-inject-alias",
    "version": "0.0.10",
    "description": "automatically generate alias based on path",
    "author": "jiangweiye <jiangweiye@outlook.com> (https://github.com/jwyGithub)",
    "license": "MIT",
    "homepage": "https://github.com/jwyGithub/vue-cli-plugin-inject-alias",
    "keywords": [
        "vue",
        "vue-cli",
        "vue-cli-plugin",
        "vue-cli-plugin-alias",
        "alias"
    ],
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "require": "./dist/index.cjs.js"
        }
    },
    "main": "./dist/index.cjs.js",
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rollup -c rollup.config.ts --configPlugin rollup-plugin-typescript2",
        "release": "npm run build && bumpp package.json --commit --push --tag && npm publish --access=public",
        "lint": "eslint ."
    },
    "peerDependencies": {
        "@jiangweiye/tsconfig": "*",
        "@vue/cli-service": "*"
    },
    "devDependencies": {
        "@jiangweiye/eslint-config": "0.0.28",
        "@jiangweiye/tsconfig": "^0.0.4",
        "@types/node": "^18.15.3",
        "@vue/cli-service": "~5.0.0",
        "bumpp": "^9.0.0",
        "eslint": "^8.36.0",
        "rollup": "^3.19.1",
        "rollup-plugin-typescript2": "^0.34.1",
        "tslib": "^2.6.0",
        "typescript": "^5.0.2"
    }
}
